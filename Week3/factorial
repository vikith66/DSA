#include<iostream>
using namespace std;
int factorial(int n){
        if(n < 1){
                return 1;
        }
        return n * factorial(n - 1);
}
int main(){
        int n;
        cout<<"Enter any Number : "<<endl;
        cin>>n;
        int result =  factorial(n);
        cout<<"Factorial Of "<<n<<" is : "<<result<<endl;
}
